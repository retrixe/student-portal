{
  "name": "student-portal",
  "version": "0.0.1",
  "description": "A student portal for my college DBMS course.",
  "repository": "https://github.com/retrixe/student-portal.git",
  "author": "Ibrahim Ansari <ansari.ibrahim1@gmail.com>",
  "license": "AGPL-3.0",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.6.0",
  "scripts": {
    "postinstall": "cd .. && husky frontend/.husky",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "yarn check && eslint --cache --cache-strategy content --cache-location .svelte-kit/ ."
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "heliodor": "^0.0.6",
    "ky": "^1.8.0",
    "phosphor-svelte": "^3.0.1",
    "remarkable": "^2.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.20.5",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/remarkable": "^2.0.8",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-svelte": "^3.5.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "sass-embedded": "^1.86.3",
    "svelte": "^5.26.2",
    "svelte-check": "^4.1.6",
    "svelte-eslint-parser": "^0.43.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.6"
  }
}
